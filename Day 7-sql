INSERT INTO Course (CourseID, CourseName, CourseFee) VALUES
(101, 'Java Programming', 5000.00),
(102, 'Python Basics', 4500.00),
(103, 'Web Development', 6000.00),
(104, 'Database Systems', 5500.00),
(105, 'Data Structures', 5200.00);


UPDATE Course
SET CourseFee = 6500.00
WHERE CourseID = 103;  
);
