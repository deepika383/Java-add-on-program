CREATE TABLE Student (
    StudentID INT PRIMARY KEY,
    Name VARCHAR(50),
    Age INT,
    Email VARCHAR(100)
);


CREATE TABLE Course (
    CourseID INT PRIMARY KEY,
    CourseName VARCHAR(100),
    CourseFee DECIMAL(10, 2)
);


INSERT INTO Student (StudentID, Name)
INSERT INTO Student (StudentID, Name, Age, Email) VALUES
